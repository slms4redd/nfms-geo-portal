{
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": 3,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    [
        "@babel/preset-env",
        {
            "useBuiltIns": "usage",
            "corejs": "3.0.0",
            "targets": {
                "esmodules": true,
                "ie": "11"
            }
        }
    ]
  ]
}
